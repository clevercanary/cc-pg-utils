{
  "compilerOptions": {
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "rootDir": "src",
    "outDir": "dist",
    "lib": [
      "es2015"
    ],
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "dist",
    "test-src",
    "node_modules"
  ]
}